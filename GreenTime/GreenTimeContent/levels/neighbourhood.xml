<?xml version="1.0" encoding="utf-8" ?>
<XnaContent>
  <!-- TODO: replace this Asset with your own XML asset data. -->
  <Asset Type="GreenTimeGameData.Components.Level">
    <name>neighbourhood</name>
    <backgroundTexture>
      <texture>neighbourhood\bg</texture>
      <position>0 0</position>
      <layer>0.75</layer>
    </backgroundTexture>
    <leftScreenName></leftScreenName>
    <rightScreenName>shop</rightScreenName>
    <gameObjects>
      <!-- Front grass -->
      <Item>
        <sprite>
          <texture>neighbourhood\grass</texture>
          <position>0 529</position>
          <layer>0.4</layer>
        </sprite>
      </Item>
      <!-- Dead tree -->
      <Item>
        <sprite>
          <texture>neighbourhood\cdtree</texture>
          <position>100 0</position>
          <layer>0.65</layer>
        </sprite>
      </Item>
      <!-- Live tree -->
      <Item>
        <sprite>
          <texture>neighbourhood\ctree</texture>
          <position>100 0</position>
          <layer>0.65</layer>
        </sprite>
        <showIf>
          <state>
            <name>puzzle_garbage_solved</name>
            <value>100</value>
          </state>
        </showIf>
      </Item>
      <!-- Garbage 1 -->
      <Item>
        <sprite>
          <texture>neighbourhood\g2</texture>
          <position>240 387</position>
          <layer>0.6</layer>
        </sprite>
        <showIf>
          <state>
            <name>puzzle_garbage_solved</name>
            <value>0</value>
          </state>
        </showIf>
        <onInteract>
          <boundX>130</boundX>
          <boundWidth>227</boundWidth>
          <text>Garbage</text>
          <transition>neighbourhood_past</transition>
          <pickUpName>garbage1</pickUpName>
        </onInteract>
      </Item>
      <!-- Garbage 2 -->
      <Item>
        <sprite>
          <texture>neighbourhood\g1</texture>
          <position>379 339</position>
          <layer>0.6</layer>
        </sprite>
        <showIf>
          <state>
            <name>puzzle_garbage_solved</name>
            <value>0</value>
          </state>
        </showIf>
        <onInteract>
          <boundX>269</boundX>
          <boundWidth>220</boundWidth>
          <text>Garbage</text>
          <transition>neighbourhood_past</transition>
          <pickUpName>garbage2</pickUpName>
        </onInteract>
      </Item>
      <!-- Garbage 3 -->
      <Item>
        <sprite>
          <texture>neighbourhood\g3</texture>
          <position>428 388</position>
          <layer>0.55</layer>
        </sprite>
        <showIf>
          <state>
            <name>puzzle_garbage_solved</name>
            <value>0</value>
          </state>
        </showIf>
        <onInteract>
          <boundX>318</boundX>
          <boundWidth>196</boundWidth>
          <text>Garbage</text>
          <transition>neighbourhood_past</transition>
          <pickUpName>garbage3</pickUpName>
        </onInteract>
      </Item>
      <!-- Bin object -->
      <Item>
        <sprite>
          <texture>neighbourhood\bin</texture>
          <position>561 553</position>
          <layer>0.4</layer>
        </sprite>
      </Item>
      <!-- Bin when object is carried -->
      <Item>
        <onInteract>
          <boundX>427</boundX>
          <boundWidth>268</boundWidth>
          <text>Bin</text>
          <dropper>
            <drops>
              <Item>garbage1</Item>
              <Item>garbage2</Item>
              <Item>garbage3</Item>
            </drops>
            <trigger>all</trigger>
            <reset>true</reset>
            <effects>
              <state>
                <name>advance_day</name>
                <value>100</value>
              </state>
            </effects>
          </dropper>
        </onInteract>
      </Item>
      <!-- dead flowers -->
      <Item>
        <sprite>
          <texture>neighbourhood/flowers</texture>
          <position>935 372</position>
          <layer>0.6</layer>
        </sprite>
      </Item>
